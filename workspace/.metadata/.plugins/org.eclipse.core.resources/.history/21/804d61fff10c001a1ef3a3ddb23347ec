package com.bridgelabz.dataStructurePrograms;
import java.util.Scanner;

import com.bridgelabz.Utility.*;
public class BankingCashCounter {

	public static void main(String[] args) 
	{
		int money=0;
		String arr[]=new String[10];
		Queue q1=new Queue();
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter list of 10 people in formate of --amount-- dep/wid(where dep-Deposit and wid-Widhral)");
		for(int i=0;i<10;i++)
		{
			q1.enqueue(sc.nextInt());
			arr[i]=sc.next();
			
		}
		money=q1.Balance(arr);
		if(money>1)
			System.out.println("Avilable amount in the bank is "+money);
		
	}
}
