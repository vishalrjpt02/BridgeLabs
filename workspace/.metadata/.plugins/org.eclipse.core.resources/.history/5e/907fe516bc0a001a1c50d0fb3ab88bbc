package com.bridgelabz.Utility;

public class Dequeue 
{
	int Queue[]=new int[10];
	int size=0;
	int front=-1;
	int rear=-1;
	public void addFront(int item)
	{
		if(rear==Queue.length)
		{
			System.out.println("queue is full");
		}
		Queue[rear]=item;
		rear++;
		size++;
	}
	public void addRear()
	{
		
	}
	public void isEmpty()
	{
		if(front==rear)
		{
			System.out.println("Queue is empty");
		}
	}
	public void Size()
	{
			
	}
}
