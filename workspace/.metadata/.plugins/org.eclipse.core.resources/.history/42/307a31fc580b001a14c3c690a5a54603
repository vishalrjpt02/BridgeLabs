package com.bridgelabz.junittesting;

import org.junit.Assert;
import org.junit.Test;

import com.bridgelabz.Utility.MyPrograms;


public class SumofdigitTest 
{
	@Test
	public void testsum()
	{
		int arr[]={3,0,-3,0,3,6,7};
		int result[]=MyPrograms.sumof3digit(arr);
		int out[]={-3,0,3};
		Assert.assertArrayEquals(out,result);
	}
	
}
